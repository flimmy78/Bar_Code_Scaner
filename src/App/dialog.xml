		<logo comment="主界面" style="STYLE_ICON | STYLE_SHORTCUT" callback="yes">
			<content_english L1="Home" L2="Options"/>
			<content_schinese L1=" " L2="选项"/>
			<jump KEY_ENTER="logo" KEY_EXT_1="scan_func" KEY_EXT_2="data_transfer_func" KEY_EXT_3="terminal_setting" KEY_EXT_4="system_info" KEY_EXT_9="test_menu"/>
		</logo>
		<scan_func comment="扫描功能" style="STYLE_ICON | STYLE_SHORTCUT" callback="yes">
			<content_english L1=" " L2="Options"/>
			<content_schinese L1="  " L2="选项"/>
			<jump KEY_ESC="logo" KEY_LEFT_SHOTCUT="scan_option_menu" KEY_EXT_9="input_send_num"/>
		</scan_func>
		<data_transfer_func comment="数据传输" callback="yes" style="STYLE_TITLE | STYLE_SHORTCUT">
			<content_english L1="Transfer Mode" L2="         Select   Esc" L3="1 BlueTooth" L4="2 USB Keyboard" L5="3 USB Virtual COM" L6="4 U Disk Mode" L7="5 Batch Mode" L8="6 Import Database" L9="7 Seperator"/>
