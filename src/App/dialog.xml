			<jump KEY_ESC="scan_option_menu"/>
		</quantity_enter_menu>
		<add_time_option comment="手动输入条码的处理界面" callback="yes" style="STYLE_TITLE | STYLE_SHORTCUT">
			<content_english L1="Add Time" L2="       Select     Esc" L3="1 Disbale" L4="2 Enable "/>
			<content_schinese L1="添加时间" L2="        选择     退出" L3="1 关闭" L4="2 开启"/>
			<jump KEY_ESC="scan_option_menu"/>
		</add_time_option>
		<add_date_option comment="是否需要添加日期" callback="yes" style="STYLE_TITLE | STYLE_SHORTCUT">
			<content_english L1="Add Date" L2="       Select     Esc" L3="1 Disbale" L4="2 Enable "/>
			<content_schinese L1="添加日期" L2="        选择     退出" L3="1 关闭" L4="2 开启"/>
			<jump KEY_ESC="scan_option_menu"/>
		</add_date_option>
		<input_barcode comment="手动输入条码的处理界面" callback="yes" style="STYLE_SHORTCUT">
			<edit x="0" y="0" style="EDIT_ALPHA" display_maxlen="20" input_maxlen="30"/>
			<jump KEY_ESC="scan_option_menu" KEY_EXT_1="scan_func"/>
		</input_barcode>
		<input_send_num comment="手动输入发送数量的处理界面" callback="yes" style="STYLE_SHORTCUT">
