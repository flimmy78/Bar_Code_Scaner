		</terminal_setting>
		<system_info comment="系统信息" style="STYLE_TITLE | STYLE_SHORTCUT" callback="yes">
			<content_english L1="Info" L2="     Select       Esc" L3="1 Memery Info" L4="2 BlueTooth Info" L5="3 Firmware Version"/>
			<content_schinese L1="系统信息 " L2="        选择     退出" L3="1 查看内存" L4="2 蓝牙信息" L5="3 版本信息"/>
